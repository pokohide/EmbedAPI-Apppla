{
  "name": "embed-api",
  "version": "1.0.0",
  "description": "Appplaの埋め込み用API",
  "main": "lambda.js",
  "files": "*.js",
  "scripts": {
    "deploy": "claudia create --name apppla-embed-api --version dev --region ap-northeast-1 --api-module lambda --profile claudia",
    "release": "claudia set-version --version production --profile claudia",
    "start": "ntl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyde2able/EmbedAPI-Apppla.git"
  },
  "keywords": [
    "Apppla",
    "Serverless",
    "AWS",
    "Lambda"
  ],
  "author": "pokohide <hyde14142@gmail.com> (https://github.com/hyde2able)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hyde2able/EmbedAPI-Apppla/issues"
  },
  "homepage": "https://github.com/hyde2able/EmbedAPI-Apppla#readme",
  "dependencies": {
    "claudia-api-builder": "^3.0.2"
  },
  "devDependencies": {
    "claudia": "^3.4.1",
    "ntl": "^2.0.0"
  }
}
